<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="com.exemplo.controller.HemogramaController">
    <TableView fx:id="tableView">
        <columns>
            <TableColumn fx:id="idColumn" text="ID" />
            <TableColumn fx:id="nomeColumn" text="Nome" />
            <!-- Adicione mais colunas conforme necessÃ¡rio -->
        </columns>
    </TableView>
    <TextField fx:id="nomeField" promptText="Nome" />
    <Button text="Adicionar" onAction="#adicionarRegistro" />
    <Button text="Listar Registros" onAction="#listarRegistros" />
</VBox>
